#include "Ht.h"
#include "PersTest04.h"

void CPersTest04::PersTest04() {
	if (PR_htValid) {
		switch (PR_htInst) {
		case TEST04_ENTRY: {
			HtContinue(TEST04_WR0);
			break;
		}
		case TEST04_WR0: {
			GW_test04_0_src_u0_data.write_addr(0, 9);
			GW_test04_0_src_u0_data.test04_0_src_u1_data.test04_0_src_v2_data = ((uint64_t)0x00000aa97390de80LL);
			P_test04_0_src_u0_data_RdAddr1 = (ht_uint1)0;
			P_test04_0_src_u0_data_RdAddr2 = (ht_uint4)9;
			HtContinue(TEST04_ST0);
			break;
		}
		case TEST04_ST0: {
			if (WriteMemBusy()) {
				HtRetry();
				break;
			}
			WriteMem_uint64_t(PR_memAddr + 0, GR_test04_0_src_u0_data.test04_0_src_u1_data.test04_0_src_v2_data);
			WriteMemPause(TEST04_LD0);
			break;
		}
		case TEST04_LD0: {
			if (ReadMemBusy()) {
				HtRetry();
				break;
			}
			ReadMem_test04_0_src_v0_data(PR_memAddr + 0);
			P_test04_0_src_u0_data_RdAddr1 = (ht_uint1)0;
			P_test04_0_src_u0_data_RdAddr2 = (ht_uint4)9;
			ReadMemPause(TEST04_CHK0);
			break;
		}
		case TEST04_CHK0: {
			if (GR_test04_0_src_u0_data.test04_0_src_u1_data.test04_0_src_v0_data != ((uint64_t)0x00000aa97390de80LL)) {
				HtAssert(0, 0);
			}
			HtContinue(TEST04_RTN);
			break;
		}
		case TEST04_RTN: {
			if (SendReturnBusy_test04()) {
				HtRetry();
				break;
			}
			SendReturn_test04();
			break;
		}
		default:
			assert(0);
		}
	}
}
