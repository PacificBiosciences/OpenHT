#include "ArgStruct.h"

struct CGV_1F {
	ht_uint9 m_x;
};

struct CGV_2F {
	ht_uint9 m_x[2];
	ht_uint9 m_y[2];
};

struct CGV_1F_AI {
#pragma htl atomic_inc
	ht_uint9 m_x;
};

struct CGV_2F_AI {
#pragma htl atomic_inc
	ht_uint9 m_x[2];
#pragma htl atomic_inc
	ht_uint9 m_y[2];
};

struct CGV_1F_AA {
#pragma htl atomic_add
	ht_uint9 m_x;
};

struct CGV_4F_AA {
#pragma htl atomic_add
	ht_uint9 m_x[4];
};

